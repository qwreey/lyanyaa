{
  "expo": {
    "name": "lyanyaa",
    "slug": "lyanyaa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./src/assets/app/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./src/assets/app/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      },
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./src/assets/app/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./src/assets/app/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/app/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
